{
  "$schema": "https://raw.githubusercontent.com/stryker-mutator/stryker-net/master/src/Stryker.CLI/Stryker.CLI/schema.json",
  "stryker-config": {
    "solution": "TheOfficeAPI.sln",
    "project": "src/TheOfficeAPI/TheOfficeAPI.csproj",
    "target-framework": "net9.0",
    "configuration": "Debug",
    "coverage-analysis": "off",
    "break-on-initial-test-failure": true,
    "verbosity": "debug",
    "reporters": [
      "html",
      "progress",
      "cleartext",
      "json"
    ],
    "thresholds": {
      "high": 80,
      "low": 60,
      "break": 0
    },
    "mutation-level": "Complete",
    "concurrency": 4,
    "ignore-methods": [
      "*ToString*",
      "*GetHashCode*",
      "*Equals*"
    ]
  }
}
