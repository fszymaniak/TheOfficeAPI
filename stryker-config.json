{
  "$schema": "https://raw.githubusercontent.com/stryker-mutator/stryker-net/master/src/Stryker.CLI/Stryker.CLI/schema.json",
  "stryker-config": {
    "solution": "TheOfficeAPI.sln",
    "project": "src/TheOfficeAPI/TheOfficeAPI.csproj",
    "target-framework": "net10.0",
    "configuration": "Debug",
    "coverage-analysis": "perTest",
    "break-on-initial-test-failure": true,
    "reporters": [
      "html",
      "progress",
      "cleartext",
      "json"
    ],
    "thresholds": {
      "high": 80,
      "low": 60,
      "break": 0
    },
    "mutation-level": "Standard",
    "since": {
      "enabled": true,
      "ignore-changes-in": [
        "tests/**"
      ],
      "target": "main"
    },
    "concurrency": 4,
    "ignore-methods": [
      "*ToString*",
      "*GetHashCode*",
      "*Equals*"
    ]
  }
}